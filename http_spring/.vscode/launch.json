    "version": "0.2.0",
    "configurations": [
        {
            "type": "java",
            "name": "Current File",
            "request": "launch",
            "mainClass": "${file}"
        },
        {
            "type": "java",
            "name": "ChatServerApplication",
            "request": "launch",
            "mainClass": "com.example.demo.chatv2.ChatServerApplication",
            "projectName": "demo"
        },
        {
            "type": "java",
            "name": "StockController",
            "request": "launch",
            "mainClass": "com.example.demo.h2_stream.StockController",
            "projectName": "demo",
            "vmArgs": "-javaagent:${workspaceFolder}/opt/newrelic/newrelic.jar"
        },
        {
            "type": "java",
            "name": "Utils",
            "request": "launch",
            "mainClass": "com.example.demo.utils.Utils",
            "projectName": "demo"
        }
    ]
}